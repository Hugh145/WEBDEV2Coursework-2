          <th scope="col">username</th>
          <th scope="col">name</th>
          <th scope="col">role</th>
          <th scope="col">typeOfUser</th>
          <th scope="col">EmailAddress</th>
        </tr>
      </thead>
      <tbody>
        {{#viewUserAccounts}}
        <tr>
          <td>{{_id}}</td>
          <td>{{user}}</td>
          <td>{{name}}</td>
          <td>{{role}}</td>
          <td>{{typeOfUser}}</td>
          <td>{{EmailAddress}}</td>
        </tr>
        {{/viewUserAccounts}}
      </tbody>
    </table>
   {{#user}}
